.contactform{
	margin-top:40px;
	width:45%;
	height:420px;
	padding:40px;
	background:#fff;
	
}
.contactform h2{
	
	font-size:30px;
	color:#333;
	font-weight:500;
}
.contactform .inputbox{
	position:relative;
	width:100%;
	margin-top:15px;
}
.contactform .inputbox input,
.contactform .inputbox textarea{
	width:100%;
	padding:10px 0;
	font-size:18px;
	margin:10px 0;
	border:none;
	border-bottom:2px solid #333;
	outline:none;
	resize:none;
}


.contactform .inputbox span{
  position:absolute;
  left:0;
  padding:5px 0;
  font-size:16px;
  margin:10px 0;
  pointer-events:none;
  transition:0.5s;
  color:#666;
}
.contactform .inputbox input:focus ~span,
.contactform .inputbox input:valid ~span,
.contactform .inputbox textarea:focus ~span,
.contactform .inputbox textarea:valid ~span
{
	color:#FFA62F;
	font-size:12px;
	transform:translateY(-15px);
}
.contactform .inputbox button[type="submit"]{
	
	width:120px;
	color:black;
	border:2px solid #333;
	cursor:pointer;
	padding:10px;
	margin-top:20px;
	border-radius:5px;
	margin-left:20px;
	font-size:20px;
	text-decoration: none;
	transition: .4s ease;
	
}
.contactform .inputbox button[type="submit"]:hover{
	
	background:#FFA62F;
}








